{
  "name": "cross-api",
  "version": "3.3.1",
  "description": "A CLI which can generate a good, clean and statically typed api structure with typescript and SOLID principles. It's features includes cruds, providers and unit tests.",
  "keywords": [
    "cross",
    "api",
    "cross-api",
    "cross api",
    "npm cross-api",
    "npm cross api"
  ],
  "author": "Mauro Domingues <maurinho.villa@hotmail.com>",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/Mauro-Domingues/cross-api"
  },
  "bugs": {
    "url": "https://github.com/Mauro-Domingues/cross-api/issues"
  },
  "bin": {
    "cross": "./bin/index.js"
  },
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files && babel index --extensions \".js,.ts\" --out-dir bin --copy-files"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
