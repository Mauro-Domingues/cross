{
  "name": "cross-api",
  "version": "5.5.9",
  "description": "A CLI which can generate a good, clean and statically typed api structure with typescript and SOLID principles. It's features includes cruds, providers, unit tests and documentation generation.",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "npx tsc && npx tsc-alias",
    "clear": "npx tsc --build --clean",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "cross": "./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mauro-Domingues/cross-api"
  },
  "bugs": {
    "url": "https://github.com/Mauro-Domingues/cross-api/issues"
  },
  "keywords": [
    "cross-api",
    "cross",
    "api",
    "cli"
  ],
  "author": "Mauro Domingues <maurinho.villa@hotmail.com>",
  "license": "Apache-2.0",
  "contributors": [],
  "files": ["src/**/*.js"],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.14.0"
  }
}
